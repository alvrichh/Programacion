package model;

import java.util.Comparator;

public class OrdenPuntos implements Comparator<AgrupacionOficial>{

	@Override
	public int compare(AgrupacionOficial o1, AgrupacionOficial o2) {
		Integer n1 = o1.getPuntosObtenidos();
		Integer n2 = o2.getPuntosObtenidos();
		return n1.compareTo(n2);
	}


}
