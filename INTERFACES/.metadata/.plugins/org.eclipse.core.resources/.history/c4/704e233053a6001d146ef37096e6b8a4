
public class Calculadora {

	private int num1;
	private int num2;

	public Calculadora(int a, int b) {
		num1 = a;
		num2 = b;
	}
	public int suma() {
		int res = num1 + num2;
		return res;
	}
	public int resta() {
		int res = num1 - num2;
		return res;
	}
	//ACTIVIDAD 3.5
	public int resta() {
		int res;
		if (resta2()) {
			res = num1 -num2;
			
		}
	}
	public int multiplica() {
		int res = num1 * num2;
		return res;
	}
	public int divide() {
		int res = num1 / num2;
		return res;
	}
	
}
