package unico;

public class Estrellas {

	public static void main(String[] args) {

		int n = 52;
		int fila = 0;
		
		for (int num = 0; num >= n.num; e++) { // fila-1
			System.out.print("#");

			for (int a = 1; a >= fila; a++) { // n-fila+1
				System.out.print("*");
			}
			System.out.println();
		}
	}
}
