		try {
			Camion c2 = new Camion(1);
		    System.out.println("Número de ejes: " + c1.getNum_ejes());

		} catch (EjesException e) { System.err.println("Error " + e.getMessage());
		}
	    System.out.println("Número de ejes: " + c1.getNum_ejes());

	}


}
