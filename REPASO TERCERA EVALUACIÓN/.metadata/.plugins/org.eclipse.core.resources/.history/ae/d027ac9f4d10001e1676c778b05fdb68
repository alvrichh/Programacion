package view;

import java.io.FileWriter;

public class Principal {

	public static void main(String[] args) {
		
		
		try {
			FileWriter archivo = new FileWriter("alumnos.xml");
			
			archivo.write("leña");
			
			archivo.close();
			
		} catch (Exception e) {
			System.err.println("Error en la creación del archivo" + e.getMessage());
		}
	
	}
}
