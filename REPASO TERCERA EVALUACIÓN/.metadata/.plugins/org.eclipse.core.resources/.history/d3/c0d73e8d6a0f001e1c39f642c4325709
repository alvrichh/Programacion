package model;

public class Libro {
	
	private String titulo;
	private String autor;
	
	
	public Libro(String titulo, String autor) {
	
		setAutor(autor);
		setTitulo(titulo);
	}
	public String getTitulo() {
		return titulo;
	}
	public void setTitulo(String titulo) {
		this.titulo = titulo;
	}
	public String getAutor() {
		return autor;
	}
	public void setAutor(String autor) {
		this.autor = autor;
	}
	
	
	

}
