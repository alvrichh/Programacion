package view;

import model.Casa;

public class Principal {

	public static void main(String[] args) {

		final String FILE_NAME = "casas.json";

		ParseJSON json = new ParseJSON();

		//LEER EL ARCHIVO JSON
		System.out.println("\nLeer un objeto casa del fichero: " + FILE_NAME +  " y mostrarlo por pantalla.");
		Casa t2 = json.leerCasa(FILE_NAME);
		System.out.println(json.printJson(t2));

	}

}
